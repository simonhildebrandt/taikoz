<%= cloudinary_js_config %>

<h2>Add a new production</h2>

<%= form_for @production, :url => casein_productions_path do |f| %>

	<%= render :partial => 'casein/productions/form', :locals => { :f => f } %>

	<p class="submits">
		<%= link_to 'Cancel', casein_productions_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Add new production", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_productions_path %></li>
<% end %>
